<%=form_with url: 'mix/new' do |f|%>
  <%=f.hidden_field 'media', value: media%> 
  <%=fields_for Mix.new do |m|%>
    Add To: <%=m.text_field 'title', list: 'title'%>
  <%end%>
  
  <datalist id="title">
  <%user.mixes.each do |m|%>
    <option value="<%=m.title%>"></option>
  <%end%>
  </datalist>
  <%=f.submit 'Add'%>
<%end%>